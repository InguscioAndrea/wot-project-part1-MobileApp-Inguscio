<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.tabbedapplication"
    >

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.MANAGE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/icona_sarc_app"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icona_sarc_app"
        android:supportsRtl="true"
        android:theme="@style/Theme.TabbedApplication"
        tools:targetApi="31"
        android:requestLegacyExternalStorage="true"
        android:preserveLegacyExternalStorage="true">

        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.TabbedApplication.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".AdministratorActivity"
            android:exported="false"
            android:label="@string/administrator_page_name">
            android:theme="@style/Theme.TabbedApplication.NoActionBar">
        </activity>

        <activity android:name=".NewUserActivity"
            android:exported="false"
            android:label="@string/administrator_page_name">
            android:theme="@style/Theme.TabbedApplication.NoActionBar">
        </activity>

        <activity android:name="PdfHelper"
            android:exported="false"
            android:label="Upload prescriptions">
            android:theme="@style/Theme.TabbedApplication.NoActionBar">
        </activity>


        <activity android:name=".PatientActivity"
            android:exported="false"
            android:label="Homepage - Patient">
            android:theme="@style/Theme.TabbedApplication.NoActionBar">
        </activity>

        <activity android:name=".NewMeasureActivity"
            android:exported="false"
            android:label="New measure">
            android:theme="@style/Theme.TabbedApplication.NoActionBar">
        </activity>

        <activity android:name=".Choose_prescriptionsActivity"
            android:exported="false"
            android:label="Prescription">
            android:theme="@style/Theme.TabbedApplication.NoActionBar">
        </activity>

        <activity android:name=".Choose_tipsActivity"
            android:exported="false"
            android:label="Tips">
            android:theme="@style/Theme.TabbedApplication.NoActionBar">
        </activity>

        <activity android:name=".Specific_tipsActivity"
            android:exported="false"
            android:label="Tips">
            android:theme="@style/Theme.TabbedApplication.NoActionBar">
        </activity>

        <activity android:name=".SinglePreviousDataPatient"
            android:exported="false"
            android:label="Data overview">
            android:theme="@style/Theme.TabbedApplication.NoActionBar">
        </activity>

        <activity android:name=".DoctorActivity"
            android:exported="false"
            android:label="Homepage - Doctor"
            tools:ignore="DuplicateActivity">
            android:theme="@style/Theme.TabbedApplication.NoActionBar">
        </activity>

        <activity android:name=".ChoosePatientActivity"
            android:exported="false"
            android:label="Choose patient"
            tools:ignore="DuplicateActivity">
            android:theme="@style/Theme.TabbedApplication.NoActionBar">
        </activity>

        <activity android:name=".ChooseMeasureActivity"
            android:exported="false"
            android:label="List of measure"
            tools:ignore="DuplicateActivity">
            android:theme="@style/Theme.TabbedApplication.NoActionBar">
        </activity>

        <activity android:name=".GraphActivity"
            android:exported="false"
            android:label="BMI Result Page"
            tools:ignore="DuplicateActivity">
            android:theme="@style/Theme.TabbedApplication.NoActionBar">
        </activity>

        <activity android:name=".RiepilogoGraficiActivity"
            android:exported="false"
            android:label="Data trend analysis"
            tools:ignore="DuplicateActivity">
            android:theme="@style/Theme.TabbedApplication.NoActionBar">
        </activity>

        <activity android:name=".Type_of_usersActivity"
            android:exported="false"
            android:label="Edit profile user"
            tools:ignore="DuplicateActivity">
            android:theme="@style/Theme.TabbedApplication.NoActionBar">
        </activity>

        <activity android:name=".List_of_usersActivity"
            android:exported="false"
            android:label="Edit profile user"
            tools:ignore="DuplicateActivity">
            android:theme="@style/Theme.TabbedApplication.NoActionBar">
        </activity>

        <activity android:name=".EditProfileActivity"
            android:exported="false"
            android:label="Edit profile user"
            tools:ignore="DuplicateActivity">
            android:theme="@style/Theme.TabbedApplication.NoActionBar">
        </activity>

        <activity android:name=".Summary_resultActivity"
            android:exported="false"
            android:label="Summary measure"
            tools:ignore="DuplicateActivity">
            android:theme="@style/Theme.TabbedApplication.NoActionBar">
        </activity>

        <activity android:name=".Hospital_informationActivity"
            android:exported="false"
            android:label="Clinical information"
            tools:ignore="DuplicateActivity">
            android:theme="@style/Theme.TabbedApplication.NoActionBar">
        </activity>

    </application>

</manifest>