<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.TabbedApplication" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#006064</item>
        <item name="colorPrimaryVariant">#FCFAF5</item>
        <item name="colorOnPrimary">#FCFAF5</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">#006064</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.TabbedApplication.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.TabbedApplication.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.TabbedApplication.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="Trasparent_button" parent="ThemeOverlay.AppCompat">
        <item name="colorAccent">#00FFFFFF</item>
    </style>
</resources>